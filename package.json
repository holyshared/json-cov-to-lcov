{
  "name": "json2lcov",
  "version": "0.1.1",
  "description": "The report data of a **json-cov** format are changed into a **lcov** format",
  "main": "lib/json2lcov.js",
  "bin": {
    "json2lcov": "./bin/json2lcov.js"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "build": "./node_modules/.bin/gulp --require coffee-script coffee",
    "coveralls": "./node_modules/.bin/gulp --require coffee-script coveralls"
  },
  "config": {
    "blanket": {
      "pattern": "json2lcov/lib"
    }
  },
  "keywords": [
    "json-cov",
    "lcov",
    "formatter",
    "converter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/holyshared/json2lcov.git"
  },
  "author": "Noritaka Horio",
  "license": "MIT",
  "readmeFilename": "README.md",
  "gitHead": "5bebff4359ac0ec7bf6faef3b69d323337fae994",
  "devDependencies": {
    "blanket": "^1.1.7",
    "chai": "^2.2.0",
    "coffee-script": "^1.9.1",
    "coveralls": "^2.10.0",
    "gulp": "^3.8.11",
    "gulp-coffee": "^2.3.1",
    "gulp-run": "^1.6.7",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-util": "^3.0.4",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2"
  }
}
